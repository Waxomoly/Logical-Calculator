            <children>
               <AnchorPane layoutX="29.0" layoutY="32.0" prefHeight="535.0" prefWidth="342.0" style="-fx-background-color: ebebeb;" />
            </children></AnchorPane>
         <AnchorPane prefHeight="600.0" prefWidth="400.0" style="-fx-background-color: #1c1d24;">
            <children>
               <ScrollPane layoutX="45.0" layoutY="43.0" prefHeight="92.0" prefWidth="310.0">
                  <content>
                     <Label fx:id="equationLabel" maxWidth="1.7976931348623157E308" minHeight="50.0" minWidth="300.0" prefHeight="79.0" prefWidth="500.0" style="-fx-background-color: #d6d6d6;">
                        <font>
                           <Font size="15.0" />
                        </font>
                     </Label>
                  </content>
               </ScrollPane>
               <Button fx:id="negasiButton" layoutX="45.0" layoutY="199.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="50.0" text="¬" textAlignment="CENTER">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="andButton" layoutX="134.0" layoutY="199.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="50.0" text="∧" textAlignment="CENTER">
