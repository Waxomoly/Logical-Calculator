               </Button>
               <Button fx:id="closingBracketButton" layoutX="305.0" layoutY="370.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="50.0" text=")" textAlignment="CENTER">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="enterButton" layoutX="220.0" layoutY="487.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="136.0" text="=" textAlignment="CENTER">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Label layoutX="303.0" layoutY="181.0" text="Backspace" textFill="WHITE" />
