                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="tButton" layoutX="305.0" layoutY="281.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="50.0" text="t" textAlignment="CENTER">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="backspaceButton" layoutX="305.0" layoutY="199.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: #de4f02;" text="&lt;-" textAlignment="CENTER">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="clearButton" layoutX="45.0" layoutY="487.0" mnemonicParsing="false" onAction="#onKeyboardClicked" prefHeight="50.0" prefWidth="136.0" style="-fx-background-color: #de4f02;" text="clear" textAlignment="CENTER">
